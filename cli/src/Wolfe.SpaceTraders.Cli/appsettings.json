{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    },
    "File": {
      "DateFormat": "yyyy-MM-dd",
      "RootPath": "C:\\SpaceTraders\\logs",
      "Files": [
        {
          "Path": "<date>.log"
        }
      ]
    }
  },
  "SpaceTraders": {
    "ApiBaseUri": "https://api.spacetraders.io/v2",
